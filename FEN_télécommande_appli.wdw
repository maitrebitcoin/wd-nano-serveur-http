   -
     name : init_télécommande
     procedure_id : 1426755691559218227
     type_code : 14
     code : |1-
      procédure init_télécommande()
      
      // pour servir le site dans .\Exe\Site_telecommande
      serveur_local.ajoute_répertoire_virtuel("/", fRepDonnées() + "\Site_telecommande")	
      
      // commandes
      serveur_local.ajoute_traitement_uri("/bouton_rouge",  ( req , réponse ) => {	
      	BTN_ROUGE.ExécuteTraitement(trtClic)
      })
      serveur_local.ajoute_traitement_uri("/bouton_vert",  ( req , réponse ) => {	
      	BTN_VERT.ExécuteTraitement(trtClic)
      })
      serveur_local.ajoute_traitement_uri("/bouton_bleu",  ( req , réponse ) => {	
      	BTN_BLEU.ExécuteTraitement(trtClic)
      })
      serveur_local.ajoute_traitement_uri("/bouton_calc",  ( req , réponse ) => {	
      	BTN_Calculatrice.ExécuteTraitement(trtClic)
      })
      
      // lancer le serveur
      num_port est un entier = 8085
      soit (bok, err ) = serveur_local.démarre_serveur(num_port)
      SI PAS bok ALORS
      	Trace(err )
      	Ferme()
      FIN
      
      // init QR code pour lancer la téélcommande
      QR_CODE					= serveur_local.url_racine(NanoServeurHttp.IPv6)
      BTN_GOTO__SITE..Libellé	= serveur_local.url_racine(NanoServeurHttp.Localhost)
     type : 458752

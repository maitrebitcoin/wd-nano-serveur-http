 code_elements :
  type_code : 1
  p_codes :
   -
     code : |1-
      PROCEDURE MaFenêtre()
      // 
      // utilise https://github.com/nimiq/qr-scanner
      
      serveur_local est NanoServeurHttp
   -
     code : |1-
      
      init_serveur()
     type : 34
   -
     type : 180
     enabled : false
   -
     type : 230
     enabled : false
  procedures :
   -
     name : init_serveur
     procedure_id : 1426478520132549398
     type_code : 14
     code : |1-
      procédure init_serveur()
      
      // page affichée du le téléphone
      serveur_local.ajoute_traitement_uri("/", renvoie_page_html_intiale)
      // lorsque le téléphone renvoie l'info
      serveur_local.ajoute_traitement_uri("/envoie_scan", _on_scan)
      	procédure interne _on_scan(requete est RequêteHttp, réponse est ReponseHttp)
      		ma_reponse est un JSON
      		ma_reponse.ok   = 1
      		réponse.envoie_chaine( ma_reponse.VersChaîne() ) 
      		// appel Ck
      		code est chaine = requete.tab_paramètre["code"]
      		ExécuteThreadPrincipal(on_scan,code)
      	fin
      // pour servir les .js, .css, image, etc.
      serveur_local.ajoute_répertoire_virtuel("/", fRepDonnées() + "\Site_scan_qrcode")	
      // lancer le serveur
      num_port est un entier = 8084
      soit (bok, err ) = serveur_local.démarre_serveur(num_port)
      SI PAS bok ALORS
      	Trace(err )
      	ferme()
      FIN
      // debut
      serveur_local.eve_avant_chaque_requete =  ( req , réponse ) => {	
      	Trace(req.get_uri() )
      }
      
      // init QR code
      QR_CODE 				= serveur_local.url_racine(NanoServeurHttp.IPv6)
      BTN_GOTO__SITE..Libellé = serveur_local.url_racine(NanoServeurHttp.Localhost)
      MaFenêtre..Plan = 1
     type : 458752
   -
     name : renvoie_page_html_intiale
     procedure_id : 1426478885204920359
     type_code : 14
     code : |1+
      procédure renvoie_page_html_intiale(requete est RequêteHttp <utile>, réponse est ReponseHttp)
      
      // renvoie la page principale
      nom_fichier_html est chaine = fRepDonnées() + "\Site_scan_qrcode\scanqr.html" 
      réponse.envoie_fichier(nom_fichier_html)
      
      // un téléphone nous a envoyée une requete ,on passe a l'étape 2
      ExécuteThreadPrincipal( ()=> { MaFenêtre..Plan = 2 } )
      
     type : 458752
   -
     name : on_scan
     procedure_id : 1426479980422359846
     type_code : 14
     code : |1+
      // appelé si un QR code a été reconnu
      procédure on_scan(code)
      MaFenêtre..Plan = 3
      SAI_SansNom1 = code
      
     type : 458752
